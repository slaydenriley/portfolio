        this.state.tags.splice(index, 1)\n    } else {\n        this.setState({\n          [event.target.name]: event.target.value\n        })\n      }\n  };\n\n  handleSubmit = (event) => {\n    event.preventDefault()\n    let formData = this.state\n    this.props.editPost(formData)\n    this.props.history.push(`/${this.state.category}s/${this.state.id}`)\n  }\n\n  handleLoading = () => {\n    if (this.props.post.requesting) {\n      return <BlockReserveLoading />;\n      console.log(\"we are requesting\")\n    }\n    else {\n      console.log(\"we are not requesting\")\n      return (\n        <div className=\"new-post\" onSubmit={this.handleSubmit}>\n          <form>\n            <div onChange={this.handleChange}>\n              <PostEditor post={this.props.post.post} tags={this.props.tags.tags}/>\n            </div>\n\n            <div className=\"rich-text-editor\">\n              <ReactQuill\n                defaultValue={this.props.post.post.content}\n                onChange={this.handleEditorChange.bind(this)}\n              />\n            </div>\n          </form>\n        </div>\n      )\n    }\n  }\n\n  render() {\n    return (\n        <div>\n          {this.handleLoading()}\n        </div>\n    )\n  }\n}\n\nconst mapStateToProps = state => {\n  return {\n    post: state.single_post,\n    tags: state.tags\n  }\n}\n\nexport default connect(mapStateToProps, {fetchSinglePost, editPost, fetchTags})(EditSinglePostContainer)\n","/Users/riley/portfolio/frontend/src/actions/deletePost.js",[],"/Users/riley/portfolio/frontend/src/components/projects/ProjectFilter.js",[],"/Users/riley/portfolio/frontend/src/reducers/tagReducer.js",[],"/Users/riley/portfolio/frontend/src/actions/fetchTags.js",[],"/Users/riley/portfolio/frontend/src/containers/posts/tagContainer.js",[],"/Users/riley/portfolio/frontend/src/components/posts/Tags.js",[],"/Users/riley/portfolio/frontend/src/actions/addNewTag.js",[],"/Users/riley/portfolio/frontend/src/containers/posts/editTagContainer.